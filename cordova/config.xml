<?xml version='1.0' encoding='utf-8'?>
<widget id="median.mobitaurus" version="0.3.0">
  <name>Mobitaurus</name>
  <description>
    Checkmk MEDIAN Mobile
  </description>
  <author email="dominik.glueck@twl-kom.de">
    Dominik.Gl√ºck
  </author>
  <platform name="android">
        <icon src="res/android/mipmap-ldpi/ic_launcher.png" density="ldpi"/>
        <icon src="res/android/mipmap-mdpi/ic_launcher.png" density="mdpi"/>
        <icon src="res/android/mipmap-hdpi/ic_launcher.png" density="hdpi"/>
        <icon src="res/android/mipmap-xhdpi/ic_launcher.png" density="xhdpi"/>
        <icon src="res/android/mipmap-xxhdpi/ic_launcher.png" density="xxhdpi"/>
        <icon src="res/android/mipmap-xxxhdpi/ic_launcher.png" density="xxxhdpi"/>
  </platform>
  
  <platform name="ios">
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-60@3x.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-60.png" width="60" height="60" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-76.png" width="76" height="76" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-76@2x.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-40.png" width="40" height="40" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-40@2x.png" width="80" height="80" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <!-- iPad -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-72.png" width="72" height="72" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-72@2x.png" width="144" height="144" />
        <!-- iPad Pro -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-167.png" width="167" height="167" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-small.png" width="29" height="29" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-small@2x.png" width="58" height="58" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-small@3x.png" width="87" height="87" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-50.png" width="50" height="50" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-50@2x.png" width="100" height="100" />
        <!-- iPad Pro -->
        <icon src="res/ios/AppIcon.appiconset/Icon-App-83.5@2x.png" width="167" height="167" />
  </platform>
  
  <preference name="EnableDeveloperConsole" value="false" />
</widget>
<plugin name="tabris-plugin-firebase" spec="3.x" />
